{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "lib"
  ]
}
